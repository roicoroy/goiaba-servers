{
  "name": "unified-auth-system",
  "version": "1.0.0",
  "description": "Unified authentication system for Medusa and Strapi",
  "scripts": {
    "test": "node tests/test-all.js",
    "test:medusa": "node tests/test-medusa-integration.js",
    "test:strapi": "node tests/test-strapi-integration.js",
    "test:cross-service": "node tests/test-cross-service.js",
    "test:security": "node tests/test-security.js",
    "docker:setup": "./docker-setup.sh",
    "docker:up": "docker-compose -f docker-compose.yml -f medusa-server/docker-compose.yml -f strapi-server/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker-compose.yml -f medusa-server/docker-compose.yml -f strapi-server/docker-compose.yml down",
    "docker:logs": "docker-compose -f docker-compose.yml -f medusa-server/docker-compose.yml -f strapi-server/docker-compose.yml logs -f",
    "docker:build": "docker-compose -f docker-compose.yml -f medusa-server/docker-compose.yml -f strapi-server/docker-compose.yml build"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "jsonwebtoken": "^9.0.2"
  },
  "keywords": [
    "authentication",
    "jwt",
    "medusa",
    "strapi",
    "unified-auth",
    "microservices"
  ],
  "author": "Unified Auth Team",
  "license": "MIT",
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}