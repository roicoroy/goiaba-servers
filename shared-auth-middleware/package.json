{
  "name": "shared-auth-middleware",
  "version": "1.0.0",
  "description": "Shared authentication middleware for Medusa and Strapi",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "start": "node dist/server.js",
    "start:dev": "ts-node src/server.ts",
    "docker:build": "docker build -t shared-auth-middleware .",
    "docker:run": "docker run -p 3000:3000 --env-file .env shared-auth-middleware",
    "docker:start": "./docker-start.sh",
    "docker:start:dev": "./docker-start.sh dev",
    "docker:start:prod": "./docker-start.sh prod",
    "docker:start:proxy": "./docker-start.sh with-proxy",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v --remove-orphans"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-node": "^10.9.0"
  }
}